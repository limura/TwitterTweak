{
  "name": "TwitterPromotionRemover",
  "description": "remove Twitter promotion tweet.",
  "version": "1.1",
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon/Icon16.png",
    "48": "icon/Icon48.png",
    "128": "icon/Icon128.png"
  },
  "manifest_version": 2
}
